<?xml version='1.0' encoding='utf-8'?>
<widget id="com.travelcompositor.online" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Travelc</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="https://online.travelcompositor.com/" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="mdpi" src="/css/clientes/default/images/manifest/logo48.png" />
        <icon density="xhdpi" src="/css/clientes/default/images/manifest/logo96.png" />
        <icon density="xxxhdpi" src="/css/clientes/default/images/manifest/logo192.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="Fullscreen" value="false" />
    <platform name="windows">
    </platform>
    <platform name="wp8">
    </platform>
    <preference name="WKWebViewOnly" value="true" />
    <plugin name="cordova-plugin-hostedwebapp" spec="^0.3.1" />
    <plugin name="cordova-plugin-wkwebview-engine" spec="^1.2.1" />
    <allow-navigation hap-rule="yes" href="https://online.travelcompositor.com/*" />
    <engine name="ios" spec="~5.1.1" />
</widget>
